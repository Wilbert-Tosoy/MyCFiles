#include <stdio.h>

int main() {
    int start, end, sum = 0;

    printf("Enter the starting number: ");
    scanf("%d", &start);
    printf("Enter the last number: ");
    scanf("%d", &end);

    for (int i = start; i <= end; i++) {
        sum = sum + i;
    }
    printf("%d", sum);
    return 0;
}
