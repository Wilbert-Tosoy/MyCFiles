#include <stdio.h>
#include <math.h>

void hypotenuseCalculator()
{
    double opposite, adjacent;

    printf("Enter the value of the opposite: ");
    scanf("%lf", &opposite);
    printf("Enter the value of the adjacent: ");
    scanf("%lf", &adjacent);

    double hypotenuse = sqrt(pow(opposite, 2) + pow(adjacent, 2));

    printf("The value of hypotenuse: %lf", hypotenuse);
}

int main(void)
{
    hypotenuseCalculator();
    return 0;
}
